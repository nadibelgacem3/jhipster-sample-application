<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity CashierReceipt.
    -->
    <changeSet id="20201105132921-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cashier_id"
                                 baseTableName="cashier_receipt"
                                 constraintName="fk_cashier_receipt_cashier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cashier"/>

        <addForeignKeyConstraint baseColumnNames="cashier_costumer_id"
                                 baseTableName="cashier_receipt"
                                 constraintName="fk_cashier_receipt_cashier_costumer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cashier_costumer"/>

    </changeSet>
</databaseChangeLog>
