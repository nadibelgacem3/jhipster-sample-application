<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Company.
    -->
    <changeSet id="20201105125421-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="company_bank_account_id"
                                 baseTableName="company"
                                 constraintName="fk_company_company_bank_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company_bank_account"/>

        <addForeignKeyConstraint baseColumnNames="company_location_id"
                                 baseTableName="company"
                                 constraintName="fk_company_company_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company_location"/>

    </changeSet>
</databaseChangeLog>
